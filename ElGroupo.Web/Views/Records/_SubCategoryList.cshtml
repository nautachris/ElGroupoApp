@using ElGroupo.Web.Models.Records
@model IEnumerable<ViewSubCategoryModel>

@{
    Layout = null;
}

<div style="text-align:center;">
    <table style="margin:0 auto; text-align:center;">
        @foreach (var item in Model)
        {
            <tr>
                <td><a asp-controller="Records" asp-action="EditSubCategory" asp-route-subCategoryId="@item.Id">@item.Name</a></td>
                <td style="margin-left:20px;"><a style="margin-left:20px;" data-remove-sub-category data-sub-category-id="@item.Id">Remove</a></td>
            </tr>

        }
    </table>
</div>