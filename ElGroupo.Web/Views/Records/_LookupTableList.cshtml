@using ElGroupo.Web.Models.Records
@model IEnumerable<ViewLookupTableModel>

@{
    Layout = null;
}

<div style="text-align:center;">
    <table style="margin:0 auto; text-align:center;">
        @foreach (var item in Model)
        {
            <tr>
                <td><a asp-controller="Records" asp-action="EditLookupTable" asp-route-lookupTableId="@item.Id">@item.Description</a></td>
                <td style="margin-left:20px;"><a style="margin-left:20px;" data-remove-lookup-table data-lookup-table-id="@item.Id">Remove</a></td>
            </tr>

        }
    </table>
</div>